package relogio;

import java.util.Scanner;

public class relogio2 {
	
	public static int retornaAnguloRelogio(int hora, int minuto) {
		
		int angulo1 = 0, angulo2 = 0;
		double angulo3 = 0, angulo4;
		angulo1 = hora*30;
		angulo2 = minuto*6;
		angulo3 = minuto*0.5;
		angulo4 = (angulo2-angulo1) - angulo3;
		
		if (hora > 12) {
			angulo4 = 360 - angulo4;
		}
		return (int)Math.abs(angulo4);
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int hora = 0;
		int minuto = 0;
		Scanner ler = new Scanner(System.in);
		hora = ler.nextInt();
		minuto = ler.nextInt();
		if (hora >= 24 || minuto >= 60) {
			System.out.println("Horário inválido.");
		}
		else {
			int angulo = retornaAnguloRelogio(hora, minuto);
			System.out.println(angulo);
		}
	}

}
